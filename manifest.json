{
  "manifest_version": 2,
  "name": "wpWeather",
  "description": "Chrome extension using bower and AngularJS",
  "version": "0.1",
  "permissions": [],
  "background": {
    "scripts": ["js/vendor/angular.min.js"]
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "web_accessible_resources" :[
      "templates/list-posts.html",
      "templates/post.html"
  ]
}